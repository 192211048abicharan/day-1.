#include <stdio.h>

int main() {
    int num, sum = 0;

    printf("Enter a number: ");
    scanf("%d", &num);


    int temp = num;  
    while (temp != 0) {
        int digit = temp % 10;
        sum += digit;        
        temp /= 10;           
    }

    printf("Sum of digits of %d: %d\n", num, sum);

    return 0;
}
